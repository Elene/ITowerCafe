@model IEnumerable<ITowerCafe.Models.FormViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>We have these kind of forms</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FormName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FormName)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.FormId })
            </td>
        </tr>
    }

</table>